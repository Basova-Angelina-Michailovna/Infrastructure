{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Information",
        "Microsoft.EntityFrameworkCore": "Warning",
        "Microsoft.AspNetCore": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThreadId"
    ],
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  },
  "Urls": "http://0.0.0.0:5033;http://0.0.0.0:8080",
  "ConnectionStrings": {
    "TestApplication": "User ID=admin;Password=admin;Host=localhost;Port=8082;Database=test-app;Max Auto Prepare=100"
  },
  "auth-service": {
    "ServiceUrl": "http://localhost:5033"
  }
}